n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n@import 'framework';\n\n$density: (\n    condensed: '3rem',\n    normal: '4rem',\n    expanded: '6rem',\n);\n\nhtml.vue-app {\n    .r-table {\n        display: flex;\n        flex-direction: column;\n        width: 100%;\n        margin: 3rem 0;\n        @include phone {\n            overflow-x: auto;\n        }\n        .r-table-empty {\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            border-bottom: 1px solid #e5e5e5;\n            @include phone {\n                border-top: 1px solid #e5e5e5;\n                margin-top: 1rem;\n                background: -theme-color('primary', 'a-90');\n            }\n        }\n\n        .r-table-after {\n            display: flex;\n            align-items: center;\n            padding: 0.5rem 1rem;\n            & > * {\n                flex-grow: 1;\n            }\n            .r-table-after-buttons {\n                display: flex;\n                justify-content: flex-end;\n            }\n\n        }\n\n        .r-table-pagination-wrapper {\n            display: flex;\n            flex-flow: row nowrap;\n            justify-content: flex-start;\n            align-items: center;\n            margin-top: 1rem;\n            padding: 0 1rem;\n\n            .r-table-pagination-before {\n                margin-right: 1rem;\n            }\n            .r-table-pagination-after {\n                margin-left: 1rem;\n            }\n\n            .r-table-pagination {\n                display: flex;\n                flex-flow: row nowrap;\n                justify-content: space-between;\n                flex: 1;\n            }\n        }\n\n        \n        .r-table-buttons-before {\n            display: flex;\n            flex-flow: row nowrap;\n            justify-content: flex-end;\n            align-items: center;\n            margin: 1rem;\n            margin-top: 0;\n\n            &-slot {\n                padding: 1rem 0;\n                @include phone {\n                    display: flex;\n                    flex-direction: column;\n                    justify-content: flex-end;\n                    & > * {\n                        margin-top: 0.5rem !important;\n                        margin-right: 0 !important;\n                    }\n                }\n            }\n\n            &-ui {\n                &:not(.--mobile) > *:not(:last-child) {\n                    padding: 0 1rem;\n                }\n                &.--mobile > *:not(:last-child) {\n                    padding: 1rem 0;\n                }\n            }\n\n            @include phone {\n                flex-direction: column;\n                align-items: flex-end;\n            }\n        }\n    }\n    &.dir\\:rtl {\n        .r-table-buttons-before-ui:not(.--mobile) > *:not(:last-child) {\n            border-right-width: 0px;\n            border-left: 1px solid #e5e5e5;\n        }\n        .r-table-grid-columns .column-header.\\--sortable h3 {\n            margin-right: 0px;\n            margin-left: 1rem;\n        }\n    }\n\n    .r-table-settings-columns {\n        display: flex;\n        flex-direction: column;\n        .inputCheck {\n            line-height: 4rem;\n            padding-left: 1rem;\n            border-bottom: 1px solid #e5e5e5;\n\n            & .inputCheck-label {\n                display: flex;\n                justify-content: space-between;\n                align-items: center;\n                padding-right: 1rem;\n            }\n\n            &:hover {\n                background: -theme-color('primary', 'a-90');\n            }\n        }\n        \n\n        @keyframes slideLeftOut {\n            0% {\n                transform: translateX(0);\n                opacity: .7;\n            }\n            100% {\n                transform: translateX(-100%);\n                opacity: 0;\n            }\n        }\n        @keyframes slideLeftIn {\n            0% {\n                transform: translateX(100%);\n                opacity: .3;\n            }\n            100% {\n                transform: translateX(0);\n                opacity: 1;\n            }\n        }\n        .slideLeft-enter-active {\n            animation: slideLeftIn $-trans-duration;\n        }\n        .slideLeft-leave-active {\n            animation: slideLeftOut $-trans-duration;\n        }\n        .slideLeft-leave-to {\n            position: absolute !important;\n            right: 0;\n        }\n\n        @keyframes slideRightOut {\n            0% {\n                transform: translateX(0);\n                opacity: .7;\n            }\n            100% {\n                transform: translateX(100%);\n                opacity: 0;\n            }\n        }\n        @keyframes slideRightIn {\n            0% {\n                transform: translateX(-100%);\n                opacity: .3;\n            }\n            100% {\n                transform: translateX(0);\n                opacity: 1;\n            }\n        }\n        .slideRight-enter-active {\n            animation: slideRightIn $-trans-duration;\n        }\n        .slideRight-leave-active {\n            animation: slideRightOut $-trans-duration;\n        }\n        .slideRight-leave-to {\n            position: absolute !important;\n            right: 0;\n        }\n    }\n\n    // Density\n    .r-table.--density\\:condensed .r-table-actions { padding: 0.5rem 0;}\n    .r-table.--density\\:normal .r-table-actions { padding: 1rem 0;}\n    .r-table.--density\\:expanded .r-table-actions { padding: 1rem 0;}\n\n    .r-table.--density\\:condensed .r-table-empty { padding: 0.5rem 1rem;}\n    .r-table.--density\\:normal .r-table-empty { padding: 1rem 1rem;}\n    .r-table.--density\\:expanded .r-table-empty { padding: 1.5rem 1rem;}\n\n    // Actions\n    .r-table-actions {\n            top: 0;\n            left: 0;\n            right: 0;\n            z-index: 3;\n            background-color: $-c-content;\n            border-top-left-radius: 4px;\n            border-top-right-radius: 4px;\n            display: flex;\n            align-items: center;\n            overflow-x: auto;\n            white-space: nowrap;\n            @at-root body.--sidebar-layout .r-table .r-table-actions {\n                top: 8rem;\n            }\n\n            .r-table-actions-select {\n                color: $-c-light;\n                padding: 0.5rem 1rem;\n            }\n\n            &.fade-leave-active {\n                position: absolute;\n            }\n        }\n}\n","//===============================================================\n//  FUNCTION: Color variator\n//  modification args should be percentage between -100% and 100%\n//===============================================================\n\n@function -color($color, $lightness: 0, $saturation: 0, $alpha: 0) {\n    $color: if(type_of($color) == string, map_get($-theme-colors, $color), $color);\n    @return scale-color($color, $lightness: $lightness, $saturation: $saturation, $alpha: $alpha);\n}\n\n//===============================================================\n//  FUNCTION: Z-index helper\n//===============================================================\n\n@function -zIndex($id) {\n    @return map_get($-zIndexes, $id);\n}\n\n//===============================================================\n//  FUNCTION: Strip units\n//===============================================================\n\n@function -stripUnits($number) {\n    @return $number / ($number * 0 + 1);\n}\n\n//===============================================================\n//  FUNCTION: Detect if number is integer\n//===============================================================\n\n@function -isInteger($number) {\n    @return $number == round($number);\n}\n\n//===============================================================\n//  FUNCTION: Floor value after needed decimal point.\n//  Example: floor-decimals(12.5983) -> 12.59\n//===============================================================\n\n@function -floorDecimals($value, $numbersAfterComa: 4) {\n    $multiplier: 1;\n    @for $i from 0 to $numbersAfterComa {\n        // $value * 10^x\n        $multiplier: $multiplier * 10;\n    }\n\n    @return floor( $value * $multiplier) / $multiplier;\n}\n\n//===============================================================\n//  FUNCTION: Calculate relative line height from px values\n//===============================================================\n\n@function -lineHeight($lineHeightPX, $fontSizePX) {\n    $lineHeight: -stripUnits($lineHeightPX / $fontSizePX);\n    @return -floorDecimals($lineHeight, 3);\n}\n\n//===============================================================\n//  FUNCTION: Calculate percentage width of children\n//  relative to its container width\n//===============================================================\n\n@function -childWidth($childWidth, $parentWidth) {\n    @return -floorDecimals($childWidth * 100% / $parentWidth);\n}\n\n//===============================================================\n//  FUNCTION: join list to string, skip null values\n//  http://hugogiraudel.com/2013/08/08/advanced-sass-list-functions/#miscellaneous\n//===============================================================\n\n@function -join($list, $separator: '') {\n    $result: ();\n    @if ($list and length($list) > 0) {\n        @for $i from 1 through length($list) {\n            $item: nth($list, $i);\n            @if type-of($item) == list {\n                $item: -join($item, $separator);\n            }\n            @if ($item != null) {\n                $result: if($i == 1, #{$item}, #{$result}#{$separator}#{$item});\n            }\n        }\n    }\n    @return $result;\n}\n\n//===============================================================\n//  FUNCTION: replace `$search` with `$replace` in `$string`\n//  https://css-tricks.com/snippets/sass/str-replace-function/\n//===============================================================\n\n@function -replace($string, $search, $replace: '') {\n    $index: str-index($string, $search);\n\n    @if $index {\n        @return -join(\n            str-slice($string, 1, $index - 1) $replace -replace(str-slice($string, $index + str-length($search)), $search, $replace)\n        )\n    }\n\n    @return $string;\n}\n\n//===============================================================\n//  FUNCTION: Replaces all : to \\:\n//===============================================================\n\n@function -formatSelector($var) {\n    $var: quote($var);\n    @return -replace($var, ':', \\:);\n}\n\n//===============================================================\n//  FUNCTION: Replaces all : to -\n//===============================================================\n\n@function -formatCssVar($prefix, $key) {\n    // replaces all : to \\:\n    $key: -replace(quote($key), ':', '-');\n\n    @return --#{$prefix}-#{$key};\n}\n\n//===============================================================\n//  FUNCTION: zips lists better than Sass native function\n//===============================================================\n\n@function -zip($list1, $list2, $seperator: 'auto') {\n    @each $item in $list2 {\n        $list1: append($list1, $item, $seperator);\n    }\n    @return $list1;\n}\n\n//===============================================================\n//  FUNCTION: Units converter\n//===============================================================\n\n@function -px($size, $base: null) {\n    @if ($-config-unitConversion and unit($size) != 'px') {\n        @if (not $base) {\n            @if (unit($size) == 'em') {\n                $base: -px($-fontSize-content, $-fontSize-base);\n            } @else if (unit($size) == 'rem') {\n                $base: $-fontSize-